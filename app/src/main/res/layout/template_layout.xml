<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="10dp"
    android:layout_marginBottom="10dp">
    <com.smacon.f2mlibrary.Button.LiquidRadioButton.LiquidRadioButton
        android:id="@+id/radio"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:gravity="center"
        app:lrb_strokeWidth="3dp"
        app:lrb_explodeCount="5"
        app:lrb_outterPadding="5dp"
        app:lrb_innerCircleRadius="5dp"
        app:lrb_strokeRadius="10dp"
        app:lrb_inAnimDuration="400"
        app:lrb_outAnimDuration="300"
        android:textColor="@color/colorGrayDark"
        app:lrb_checkedColor="@color/f2mcolor"
        app:lrb_unCheckedColor="@color/f2mcolor"
        android:text="Test 3"
        android:textStyle="bold"
        android:padding="5dp"
        android:layout_margin="5dp"/>
    <View
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorPrimary"/>

</LinearLayout>